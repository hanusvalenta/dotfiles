{
   "action": {
      "default_icon": {
         "128": "images/icon_128.png",
         "16": "images/icon_16.png",
         "32": "images/icon_32.png",
         "48": "images/icon_48.png",
         "64": "images/icon_64.png",
         "96": "images/icon_96.png"
      },
      "default_title": "Text Blaze"
   },
   "background": {
      "service_worker": "js/background.js",
      "type": "module"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+Space"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/inPageNotifier.js", "js/contentScript.js" ],
      "match_about_blank": true,
      "match_origin_as_fallback": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_idle"
   } ],
   "content_security_policy": {
      "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals allow-popups-to-escape-sandbox; script-src 'self' 'unsafe-inline' 'unsafe-eval'; child-src 'self';"
   },
   "description": "Create smart text expansions to get more done and streamline your work. Billions of keystrokes saved!",
   "externally_connectable": {
      "matches": [ "https://*.blaze.today/*", "https://blaze.today/*" ]
   },
   "homepage_url": "https://blaze.today",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "images/icon_128.png",
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "64": "images/icon_64.png",
      "96": "images/icon_96.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "135",
   "name": "Text Blaze: Templates and Snippets",
   "offline_enabled": true,
   "omnibox": {
      "keyword": "tb"
   },
   "permissions": [ "tabs", "storage", "unlimitedStorage", "clipboardRead", "clipboardWrite", "contextMenus", "offscreen", "scripting" ],
   "sandbox": {
      "pages": [ "html/sandbox.html", "html/formLoader.html" ]
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.3.0",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "js/tbremapper.js", "images/icon_128.png", "images/icon_128_white_bottom.png", "css/widget.css", "css/picker.css", "css/notifier.css" ]
   } ]
}
